{
    "name": "msgphp/msgphp",
    "description": "Combined MsgPHP package for CI purposes",
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/orm": "^2.6",
        "doctrine/persistence": "^1.4",
        "ramsey/uuid": "^3.7",
        "symfony/config": "^3.4 || ^4.4 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.4 || ^5.0",
        "symfony/form": "^3.4.24 || ^4.4 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.4 || ^5.0",
        "symfony/messenger": "^4.4 || ^5.0",
        "symfony/security-core": "^3.4 || ^4.4 || ^5.0",
        "symfony/validator": "^3.4 || ^4.4 || ^5.0",
        "symfony/var-exporter": "^4.4 || ^5.0"
    },
    "require-dev": {
        "ramsey/devtools": "^1.7",
        "ramsey/devtools-lib": "1.3.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "captainhook": {
            "force-install": true
        },
        "ramsey/conventional-commits": {
            "configFile": "conventional-commits.json"
        },
        "ramsey/devtools": {
            "command-prefix": "dev"
        }
    },
    "autoload": {
        "psr-4": {
            "MsgPhp\\": "src/",
            "MsgPhp\\User\\": "src/UserEav/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
